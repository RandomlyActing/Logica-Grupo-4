programa {
  funcao inicio() {
    inteiro numero1, numero2
    escreva ("Escreva um numero: ")
    leia (numero1)
    escreva ("Escreva outro numero: ")
    leia (numero2)


    escreva ("A multiplicação deles é: ", numero1 * numero2, "\nA divisão deles é: ", numero1 / numero2, "\nA subtração deles é: ", numero1 - numero2, "\nA adição deles é: ", numero1 + numero2)
  }
}
